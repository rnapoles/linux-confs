objectclass ( 1.3.6.1.4.1.10018.1.2.1 NAME 'ServicesAccount' SUP top AUXILIARY
	DESC 'Servicios de Usuario  '
	MUST ( mail $ homeDirectory )
        MAY ( uidNumber $ gidNumber $ uid $ cn $ loginShell $ userPassword $ userPasswkey $ NumeroCarnet
        $ ServProxyAccess $ ServProxyIp $ ServProxyMac $ ServProxyQuota $ ServProxyQuotaFija $ ServMailIn 
        $ ServMailOut $ ServMailList $ ServMailQuota $ ServXmmpAccess  $ Sexo $ Area 
        $ CategDoc $ CategCient $ Administrador $ Cuadro $ Militante $ Expiracion
	$ Calle $ Numero $ Apartamento $ Entre $ Y $ Reparto $ Provincia $ Municipio $ ServAccountStatus 
	$ ServProxyStatus $ ServMailStatus $ NumeroSolapin $ NumeroPin $ Hosting 
	$ ServMailRestrictions $ bccmaps $ Localizacion $ cargo $ aseo
	 ) )


